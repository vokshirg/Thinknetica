<tr>
  <td><%= result.number %></td>
  <td><%= result.route.name %></td>
  <td>
    <%= form_tag fill_ticket_search_path, method: :post do %>
      <%= hidden_field_tag :end_station_id, @end_station.id %>
      <%= hidden_field_tag :start_station_id, @start_station.id %>
      <%= hidden_field_tag :train_id, result.id %>
      <%= hidden_field_tag :passanger_id, Passanger.first.id %>
      <%= submit_tag 'Купить' %>
    <% end %>

  </td>
</tr>
